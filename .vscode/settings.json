{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/*.js.map": {
            "when": "$(basename)"
        }
    },
    "json.schemas": [
        {
            "fileMatch": [
                "/test/queries/**"
            ]
        }
    ]
}
