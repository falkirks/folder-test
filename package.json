{
    "name": "@ubccpsc310/folder-test",
    "description": "Load many tests from a folder",
    "homepage": "https://github.com/falkirks/folder-test",
    "contributors": [
        "Noa Heyl",
        "Braxton Hall"
    ],
    "license": "GPL-3.0",
    "version": "2.0.0",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "engines": {
        "node": ">= 16 < 17"
    },
    "devDependencies": {
        "@types/chai": "4.2.21",
        "@types/fs-extra": "9.0.12",
        "@types/mocha": "9.0.0",
        "@types/node": "16.11.7",
        "@typescript-eslint/eslint-plugin": "5.9.0",
        "@typescript-eslint/eslint-plugin-tslint": "5.9.0",
        "@typescript-eslint/parser": "5.9.0",
        "eslint": "8.6.0",
        "nyc": "15.1.0",
        "ts-node": "10.2.1",
        "tslint": "6.1.3",
        "typescript": "4.5.4"
    },
    "dependencies": {
        "chai": "4.3.4",
        "fs-extra": "10.0.0",
        "mocha": "9.1.0"
    },
    "scripts": {
        "cover": "nyc --reporter text --reporter html yarn run test",
        "build": "tsc && yarn lint",
        "test": "mocha --require ts-node/register test/**/*.spec.ts",
        "lint": "eslint src test --ext .ts"
    }
}
