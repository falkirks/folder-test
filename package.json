{
    "name": "folder-test",
    "description": "Load many tests from a folder",
    "homepage": "https://github.com/falkirks/folder-test",
    "author": "",
    "license": "GPL-3.0",
    "version": "1.0.0",
    "engines": {
        "node": ">= 14 < 15"
    },
    "devDependencies": {
        "@types/chai": "4.2.12",
        "@types/fs-extra": "5.0.4",
        "@types/mocha": "2.2.46",
        "@types/node": "12.12.6",
        "@typescript-eslint/eslint-plugin": "^2.14.0",
        "@typescript-eslint/eslint-plugin-tslint": "^2.14.0",
        "@typescript-eslint/parser": "^2.14.0",
        "eslint": "^6.8.0",
        "nyc": "15.1.0",
        "ts-node": "4.1.0",
        "tslint": "5.18.0",
        "typescript": "3.7.4"
    },
    "dependencies": {
        "chai": "4.1.2",
        "fs-extra": "7.0.0",
        "mocha": "^8.1.3"
    },
    "scripts": {
        "cover": "nyc --reporter text --reporter html yarn run test",
        "build": "tsc && yarn lint",
        "test": "mocha --require ts-node/register --timeout 10000 test/**/*.spec.ts",
        "lint": "eslint . --ext .ts"
    }
